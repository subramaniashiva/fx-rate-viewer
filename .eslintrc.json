{
  "extends": [
    "google",
    "plugin:react/recommended",
    "plugin:flowtype/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["flowtype"],
  "rules": {
    "require-jsdoc": 0,
    "flowtype/no-types-missing-file-annotation": 0
  },
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "16.0"
    }
  }
}